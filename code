fibonacci = [0, 1]
try:
    S = input("Digite um número maior ou igual a 2: ")
    n = int(S)
    
except:
        print("Deve ser um número maior ou igual a 2!")
else:
    if n < 2:
       print("Deve ser um número maior ou igual a 2!")  
    else:
        print("Os {} primeiros termos da sequência de Fibonacci são: ".format(n))
        for i in range(2, n):
            fibonacci.append(fibonacci[i-1] + fibonacci[i-2])
        for i in range(n):
            print(fibonacci[i], end=" ")
